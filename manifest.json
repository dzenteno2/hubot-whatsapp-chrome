{
  "name": "Gus Whatsapp Logger",
  "version": "0.1.0",
  "icons": {
    "16": "gus.jpg",
    "48": "gus.jpg",
    "128": "gus.jpg"
  },
  "permissions" : [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["*://web.whatsapp.com/*"]
  },
  "browser_action": {
    "default_title": "Gus Whatsapp Logger"
  },
  "manifest_version": 2
}
